#!/bin/bash

cd `dirname $0`

if [ $(dpkg-query -W -f='${Status}' graphviz 2>/dev/null | grep -c "ok installed") -eq 0 ];
then
  echo "Graphviz is not installed. This package provides /usr/bin/dot used by plantuml.jar to produce the UML diagrams"
fi

java -jar ./plantuml.jar internal/*.uml
cp internal/getObjectStream.png ../server/static/js/getObjectStream.png
cp internal/etag.png            ../server/static/js/etag.png
